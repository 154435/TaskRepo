@model Restaurant.web.Models.FoodCategory

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>FoodCategory</h4>
<hr />
<div class="row">
    <div class="col-md-4" style="background-color:#BFFCD4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group" style="background-color:#E79EF3 ">
                <label asp-for="FoodcategoryName" class="control-label">Food Category Name</label>
                <input asp-for="FoodcategoryName" class="form-control" />
